<p id="notice"><%= notice %></p>

<h1>Rankings</h1>

<table>
  <thead>
    <tr>
      <th>Points</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rankings.each do |ranking| %>
      <tr>
        <td><%= ranking.points %></td>
        <td><%= ranking.user %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
